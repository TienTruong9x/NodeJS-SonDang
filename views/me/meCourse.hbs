<h1>Khóa học của tôi : </h1>
<table class="table mt-4 mb-4">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Tên khóa học</th>
      <th scope="col">Trình độ</th>
      <th scope="col" colspan="3">Thời gian tạo</th>

    </tr>
  </thead>
  <tbody>
    {{#each courses}}
    <tr>
      <th scope="row">{{increase @index}}</th>
      <td>{{this.name}}</td>
      <td>{{this.level}}</td>
      <td>{{this.createdAt}}</td>
      <td><a name="" id="" class="btn btn-warning" href="#" role="button">Sửa</a></td>
      <td><a name="" id="" class="btn btn-danger" href="#" role="button">Xóa</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>