{
    "name": "course",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon --inspect ./bin/www",
        "css": "node-sass public/stylesheets/style.scss public/stylesheets/style.css --watch",
        "beautiful": "prettier --single-quote --trailing-comma all --write --tab-width 4 {*,**}.{js,json,scss}"
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm run beautiful"
      }
      },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "hbs": "~4.0.4",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2"
    }
}
